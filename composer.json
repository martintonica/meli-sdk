{
    "name": "tecnogo/meli-sdk",
    "description": "A better SDK for Mercadolibre API",
    "homepage": "https://www.tecnogo.com",
    "keywords": ["sdk", "mercadolibre", "api"],
    "authors": [
        {
            "name": "Valentin Starck",
            "email": "valentin.starck@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Tecnogo\\MeliSdk\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tecnogo\\MeliSdk\\Test\\": "tests/MeliSdk/Test/"
        }
    },
    "require": {
        "php": ">=7.2.0",
        "ext-curl": "*",
        "ext-json": "*",
        "psr/simple-cache": "^1.0",
        "symfony/cache": "^4.2",
        "illuminate/container": "v5.8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true
}
